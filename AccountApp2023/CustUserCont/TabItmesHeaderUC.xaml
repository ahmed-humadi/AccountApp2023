<UserControl x:Class="AccountApp2023.CustUserCont.TabItmesHeaderUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AccountApp2023.CustUserCont"
             mc:Ignorable="d" Height="auto" Width="auto" FlowDirection="RightToLeft">
    <StackPanel Orientation="Horizontal">
        <Button x:Name="TabItem_CloseBtn" Background="White" BorderBrush="Transparent">
            <Button.Content>
                        <TextBlock Text="&#xE10A;" FontFamily="Segoe MDL2 Assets"/>
            </Button.Content>
        </Button>
        <TextBlock x:Name="TabItem_IcontxtBx" Margin="0,0,0,0" FontFamily="Segoe MDL2 Assets"></TextBlock>
        <TextBlock x:Name="TabItem_NametxtBx"></TextBlock>
    </StackPanel>
</UserControl>
